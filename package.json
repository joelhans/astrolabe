    "autoprefixer": "^10.1.0",
    "gray-matter": "^4.0.2",
    "image-size": "0.9.3",
    "next": "10.0.8",
    "next-mdx-remote": "^2.0.0",
    "next-seo": "4.17.0",
    "next-themes": "^0.0.10",
    "postcss": "^8.2.1",
    "preact": "^10.5.7",
    "prism-react-renderer": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-select": "^4.3.0",
